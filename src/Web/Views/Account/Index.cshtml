@{
    Layout = ViewData["Layout"].ToString();
}




<hr/>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-success">
            <div class="panel-heading">
                系统提示：
            </div>
            <div class="panel-body">
                <ul>
                    <li>@("当前登录用户:" + ViewData["UserID"] )</li>
                    <li>@("登陆时间：" + DateTime.Now)</li>
                    <li>请在左边开始您的工作</li>

                </ul>
                <p></p>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-lg-12">
        <div class="alert alert-warning alert-dismissable">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>

            @ViewData["msg"]
            &nbsp; &nbsp; &nbsp; &nbsp;
            @Html.ActionLink("重新登录", "Login", new RouteValueDictionary(), new { @class = "btn btn-primary" })
            &nbsp; &nbsp;
            @Html.ActionLink("退出登录", "LoginOut", new RouteValueDictionary(), new { @class = "btn btn-warning" })
        </div>
    </div>

</div>